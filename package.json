{
  "name": "klus-hc",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node ./bin/www",
    "dev": "nodemon  --trace-warnings ./bin/www"
  },
  "nodemonConfig": {
    "restartable": "rs",
    "ignore": [
      "node_modules/",
      "build/"
    ],
    "delay": "100",
    "env": {
      "NODE_ENV": "development"
    },
    "ext": "js,www"
  },
  "dependencies": {
    "bcrypt": "^5.1.1",
    "cookie-parser": "~1.4.4",
    "cors": "^2.8.5",
    "debug": "~2.6.9",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "helmet": "^7.1.0",
    "http-errors": "~1.6.3",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.0.1",
    "morgan": "~1.9.1",
    "openai": "^4.19.0",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "nodemon": "^3.0.1"
  }
}
